<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
//set polygons for bright and dark sides of Earth
for (int i = 0; i < np; i++)
{
  theta = i*Math.PI/(np-1) - Math.PI/2.0;
  el[i][0] = rE*Math.cos(theta);
  el[i][1] = rE*Math.sin(theta);
  ed[i][0] = rE*Math.cos(theta + Math.PI);
  ed[i][1] = rE*Math.sin(theta + Math.PI);
  }
  

]]></Code>
</Content>
</Osejs.Model.Initialization.Page>
