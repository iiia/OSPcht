<Osejs.Model.Constraints>
<Osejs.Model.Constraints.Page>
<Type>CODE_EDITOR</Type>
<Name>Shadow calculation</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
dec=Math.toRadians(23.5)*Math.sin(2*Math.PI*day/365.25);
angle=latitude-dec;
if(Math.abs(angle)>=Math.PI/2.0){
  shadowVis=false;
}else{
  shadowVis=true;
}
if(latitude<0 && !verticalGnomon){
  gnomonHeight=-Math.abs(gnomonHeight);
}else{
  gnomonHeight=Math.abs(gnomonHeight);
}
if(verticalGnomon){ // gnomon vertical; dial horizontal
  shadowLength=gnomonHeight*Math.tan(angle);
  rayLength=gnomonHeight/Math.cos(angle);
  gnomonX=(rEarth+gnomonHeight/zoom)*Math.cos(latitude);
  gnomonY=(rEarth+gnomonHeight/zoom)*Math.sin(latitude);
}else{  // gnomon axial; dial horizontal
  shadowLength=gnomonHeight*Math.cos(dec)/Math.cos(angle);
  rayLength=gnomonHeight*Math.sin(latitude)/Math.cos(angle);
  gnomonX=rEarth*Math.cos(latitude);
  gnomonY=rEarth*Math.sin(latitude)+gnomonHeight/zoom;
}

]]></Code>
</Content>
</Osejs.Model.Constraints.Page>
