<Osejs.Model.Evolution>
<Osejs.Model.Evolution.Page>
<Type>ODE_EDITOR</Type>
<Name>Equations</Name>
<Active>true</Active>
<Content>
<PreliminaryCode>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
double deltaX=x2-x1;                        // x12 separation
double deltaY=y2-y1;                        // y12 separation
double dr_2=(deltaX*deltaX+deltaY*deltaY);  // r12 squared
double dr_3=Math.sqrt(dr_2)*dr_2;           // r12 cubed

double r1_2=x1*x1+y1*y1;                    // r1 squared
double r1_3=r1_2*Math.sqrt(r1_2);           // r1 cubed
      
double ax1=-2*x1/r1_3-deltaX/dr_3;          //vx1 rate
double ay1=-2*y1/r1_3-deltaY/dr_3;          //vy1 rate

double r2_2=x2*x2+y2*y2;                    // r2 squared
double r2_3=r2_2*Math.sqrt(r2_2);           // r2 cubed

double ax2=-2*x2/r2_3+deltaX/dr_3;          //vx2 rate
double ay2=-2*y2/r2_3+deltaY/dr_3;          //vy2 rate

]]></Code>
</PreliminaryCode>
<IndependentVariable>t</IndependentVariable>
<Increment>dt</Increment>
<Increment>dt</Increment>
<Rate state="x1">vx1</Rate>
<Rate state="vx1">ax1</Rate>
<Rate state="y1">vy1</Rate>
<Rate state="vy1">ay1</Rate>
<Rate state="x2">vx2</Rate>
<Rate state="vx2">ax2</Rate>
<Rate state="y2">vy2</Rate>
<Rate state="vy2">ay2</Rate>
<Method>RungeKuttaFehlberg</Method>
<Tolerance>1.0e-6</Tolerance>
<Comment><![CDATA[Newton's Law applied to the classical helium model]]></Comment>

</Content>
