<Type>LIBRARY_EDITOR</Type>
<Name>Compute Potential Message</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
public void computeMessage () {
  double v=0;
  double r=0;
  double x=_view.drawingPanel.getMouseX();
  double y=_view.drawingPanel.getMouseY();
  if(sphere) {
    r=Math.sqrt(x*x+y*y+zPlane*zPlane);
  }
  else {
    r=Math.sqrt(x*x+y*y);
  }
  if (r<1.0) {
    v=12;
  }
  else if (r>10.0){
    v=0;
    }
  else {
    if(sphere) {
      v=13.3*(1/r-.1); //v in Volts, r in cm
    }
    else {
      v=5.2*Math.log(10/r); //v in Volts, r in cm
    }
  }
  msgStr= "V(x,y)="+_view.format(v,"0.00");
}
]]></Code>
</Content>
</Osejs.Model.Library.Page>
<Osejs.Model.Library.Page>
<Type>LIBRARY_EDITOR</Type>
<Name>Compute Force</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
public void computeForce () {
  double f=0;
  double r=0;
  if(sphere) {
    r=Math.sqrt(xq*xq+yq*yq+zq*zq);
  }
  else {
    r=Math.sqrt(xq*xq+yq*yq);
  }
  if (r<1.0) {
    f=0;
  }
  else if (r>10.0){
    f=0;
    }
  else {
    if(sphere) {
      f=1330/(r*r); //r in units of cm and F in units of 10-12 N
    }
    else {
      f=520/r;  //r in units of cm and F in units of 10-12 N
    }
  }
  forceStr= "F="+_view.format(f,"0");
}
]]></Code>
</Content>
</Osejs.Model.Library.Page>
