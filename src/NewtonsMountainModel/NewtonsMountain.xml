<Osejs.Model.Library>
<Osejs.Model.Library.Page>
<Type>LIBRARY_EDITOR</Type>
<Name>getAccels</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
//computes x-component of projectile's acceleration
double getAx (double x, double y) {
  if ((!point) && (x*x+y*y < 1)) {
    return -G*x;}
  else {
    return -G*x/Math.pow(x*x+y*y,1.5);}
}

//computes y-component of projectile's acceleration
double getAy (double x, double y) {
  if ((!point) && (x*x+y*y < 1)) {
    return -G*y;}
  else {
    return -G*y/Math.pow(x*x+y*y,1.5);}
}

]]></Code>
</Content>
</Osejs.Model.Library.Page>
<Osejs.Model.Library.Page>
<Type>LIBRARY_EDITOR</Type>
<Name>hitEarth</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
//function to determine if projectile has hit earth
//(used in ODE event handling)
double hitEarth (double x, double y) {
  if (!pass) {
    return x*x+y*y-1;}
  else {
    return 100;}
}
]]></Code>
</Content>
</Osejs.Model.Library.Page>
<Osejs.Model.Library.Page>
<Type>LIBRARY_EDITOR</Type>
<Name>customInit</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
//resets particle to mountain top with new values and starts new trace
public void customInit () {
  _view.ballTrail.newSegment();
  t = 0;
  x = 0.1;
  y = 1.185;
  vx = vi*Math.cos(theta*Math.PI/180.0)/6.378e3;
  vy = vi*Math.sin(theta*Math.PI/180.0)/6.378e3;
  _view.update();
}
]]></Code>
</Content>
