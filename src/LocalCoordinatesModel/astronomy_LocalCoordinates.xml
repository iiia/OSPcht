<Type>LIBRARY_EDITOR</Type>
<Name>setColors</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
//set opaque colors if not using transparency while running
public void setColors () {
  if (useTrans) {
    eqColor = new Color(255,0,255,50);
    eclColor = new Color(255,0,0,50);
    horColor = new Color(0,255,0,100);}
  else {
    eqColor = new Color(255,0,255,255);
    eclColor = new Color(255,0,0,255);
    horColor = new Color(0,255,0,255);}
}
 
//set colors to transparent while not running
public void setTrans () {
    eqColor = new Color(255,0,255,50);
    eclColor = new Color(255,0,0,50);
    horColor = new Color(0,255,0,100);
}
]]></Code>
</Content>
